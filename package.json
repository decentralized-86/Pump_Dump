{
  "name": "solpump",
  "version": "1.0.0",
  "description": "Telegram-based web game with Solana integration",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "pkg -t node12-win-x64 --output solpump server.js",
    "test": "jest"
  },
  "keywords": [
    "solana",
    "telegram",
    "game",
    "web3"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@solana/spl-token": "^0.3.9",
    "@solana/web3.js": "^1.98.2",
    "@types/bs58": "^4.0.4",
    "axios": "^1.10.0",
    "bs58": "^6.0.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "ioredis": "^5.6.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "redis": "^4.6.11",
    "telegraf": "^4.16.3",
    "tweetnacl": "^1.0.3",
    "twitter-api-sdk": "^1.2.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/solpump.git"
  },
  "pkg": {
    "targets": [
      "node12-win-x64"
    ],
    "assets": [
      "services/**/*",
      "routes/**/*",
      "models/**/*",
      "config/**/*",
      "public/**/*",
      "node_modules/**/*"
    ]
  }
}
